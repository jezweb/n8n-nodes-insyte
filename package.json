{
  "name": "n8n-nodes-insyte",
  "version": "0.1.0",
  "description": "n8n node for Insyte CRM API (Window Furnishings Industry)",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "insyte",
    "crm",
    "window-furnishings"
  ],
  "license": "MIT",
  "homepage": "https://github.com/jezweb/n8n-nodes-insyte",
  "author": {
    "name": "Jezweb",
    "email": "jeremy@jezweb.net"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jezweb/n8n-nodes-insyte.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/InsyteApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Insyte/Insyte.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-core": "*",
    "n8n-workflow": "*",
    "prettier": "^2.8.1",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
